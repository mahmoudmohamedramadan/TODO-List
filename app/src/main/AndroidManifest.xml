<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mahmoudramadan.todo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_todo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_todo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TODOList">
        <activity android:name="com.mahmoudramadan.todo.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.mahmoudramadan.todo.MainActivity" />
        <activity android:name="com.mahmoudramadan.todo.TasksActivity" />
    </application>

</manifest>